{
  "name": "dirty-dom",
  "version": "4.1.0",
  "description": "Micro library for handling dirty DOM updates",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --config config/build.conf.ts",
    "build:demo": "webpack --config demo/config/dev.conf.ts --port 8080",
    "demo": "npm run build -- --watch & NODE_ENV=development webpack-dev-server --inline --hot --config demo/config/dev.conf.ts --port 8080",
    "test": "echo \"No tests yet\"",
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/dirty-dom.git"
  },
  "keywords": [
    "dirty",
    "dom",
    "updates"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "files": [
    "build"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/compression-webpack-plugin": "^2.0.1",
    "@types/debug": "^4.1.5",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.8",
    "@types/webpack": "^4.41.10",
    "@types/webpack-env": "^1.15.1",
    "@types/webpack-node-externals": "^1.7.1",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "debug": "^4.1.1",
    "html-webpack-plugin": "^4.0.4",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "types": "build/index.d.ts",
  "dependencies": {
    "spase": "^4.0.0"
  }
}
